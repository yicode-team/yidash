{
    "name": "yidash",
    "version": "1.0.6",
    "description": "基于 lodash 函数库扩展的实用业务方法库",
    "scripts": {
        "build": "del-cli --force dist & microbundle",
        "dev": "del-cli --force dist & microbundle watch",
        "doc": "del-cli --force docs & jsdoc -c jsdoc.config.json -t ./node_modules/better-docs",
        "patch": "patch-package",
        "go": "np"
    },
    "keywords": [],
    "type": "module",
    "author": "chensuiyi <bimostyle@qq.com>",
    "license": "ISC",
    "source": "lib/index.js",
    "main": "dist/yidash.cjs",
    "exports": {
        "require": "./dist/yidash.cjs",
        "import": "./dist/yidash.module.js",
        "default": "./dist/yidash.modern.js"
    },
    "module": "./dist/yidash.module.js",
    "umd:main": "./dist/yidash.umd.js",
    "engines": {
        "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
    },
    "devDependencies": {
        "better-docs": "2.7.1",
        "del-cli": "^4.0.1",
        "jsdoc": "^3.6.9",
        "lodash-es": "^4.17.21",
        "microbundle": "^0.14.2",
        "np": "^7.6.0",
        "patch-package": "^6.4.7"
    }
}
